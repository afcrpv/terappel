= f.input :ass_med_proc, as: :radio_buttons, collection: Dossier::ONI, item_wrapper_class: "inline"

%table.grsant.span5
  %thead
    %tr
      %th{colspan: "5", style: "text-align: right;"} Grossesses Antérieures
      %th= f.text_field :grsant, class: "span1"
  %tbody
    %tr
      - [:fcs, :geu, :miu].each do |item|
        %th= item.to_s.upcase
        %td= f.text_field item, class: "span1"
    %tr
      - [:ivg, :img, :nai].each do |item|
        %th= item.to_s.upcase
        %td= f.text_field item, class: "span1"

%table.grossesse_dates
  %tr
    %th Age grossesse (SA)
    %td.span8= f.text_field :age_grossesse, class: "span1"
  %tr
    %th DDR
    %td= f.text_field :date_dernieres_regles, class: "span3 dateITA dateNotFuture"
  %tr
    %th Debut de grossesse
    %td= f.text_field :date_debut_grossesse, class: "span3 dateITA dateNotFuture"
  %tr
    %th Date accouchement prévu
    %td= f.text_field :date_accouchement_prevu, class: "span3 dateITA"
  %tr
    %td#grossesse_date_messages
    %td
      = link_to "Calculer", "#", id: "calc_dates_grossesse", class: "btn"
      = link_to "Effacer", "#", id:"reset_dates_grossesse", class: "btn"
